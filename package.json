{
  "name": "front-2023-pizzeria-belsay",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:dev": "ng build --stats-json && npx webpack-bundle-analyzer dist/front_2023_pizzeria_belsay/stats.json",
    "build": "ng build --configuration production --aot",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false",
    "lint": "eslint src/**/*.{ts,html} --fix",
    "lint:ci": "eslint src/**/*.{ts,html}",
    "compodoc": "npx compodoc -p tsconfig.doc.json",
    "precommit": "npm run lint && npm run test && npm run compodoc"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/template-parser": "^15.1.0",
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "bootstrap": "^5.2.3",
    "eslint-config-angular": "^0.5.0",
    "jquery": "^3.6.3",
    "popper.js": "^1.16.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.3",
    "@angular-eslint/eslint-plugin": "^15.1.0",
    "@angular-eslint/eslint-plugin-template": "^15.1.0",
    "@angular/cli": "~14.2.3",
    "@angular/compiler-cli": "^14.2.0",
    "@compodoc/compodoc": "^1.1.19",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.8.1",
    "prettier-eslint": "^15.0.1",
    "typescript": "~4.7.2",
    "webpack-bundle-analyzer": "^4.7.0"
  }
}
